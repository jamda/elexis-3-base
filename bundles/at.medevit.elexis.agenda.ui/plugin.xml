<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="ch.elexis.agenda.Views"
            class="at.medevit.elexis.agenda.ui.view.WeekView"
            id="at.medevit.elexis.agenda.ui.view.week"
            name="Week"
            restorable="true">
      </view>
      <view
            category="ch.elexis.agenda.Views"
            class="at.medevit.elexis.agenda.ui.view.ParallelView"
            id="at.medevit.elexis.agenda.ui.view.parallel"
            name="Parallel"
            restorable="true">
      </view>
      <view
            category="ch.elexis.agenda.Views"
            class="at.medevit.elexis.agenda.ui.view.AgendaView"
            id="at.medevit.elexis.agenda.ui.view.agenda"
            name="Agenda Web"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="at.medevit.elexis.agenda.ui.command.setStatus"
            name="SetStatus">
         <commandParameter
               id="at.medevit.elexis.agenda.ui.command.parameter.statusId"
               name="statusId"
               optional="false">
         </commandParameter>
      </command>
      <command
            id="at.medevit.elexis.agenda.ui.command.edit"
            name="Edit">
      </command>
      <command
            id="at.medevit.elexis.agenda.ui.command.delete"
            name="Delete">
      </command>
      <command
            id="at.medevit.elexis.agenda.ui.command.settopcontrol"
            name="SetTopControl">
         <commandParameter
               id="at.medevit.elexis.agenda.ui.command.parameter.controlId"
               name="controlId"
               optional="false">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:at.medevit.elexis.agenda.ui.week">
         <menu
               label="Status">
            <dynamic
                  class="at.medevit.elexis.agenda.ui.menu.SetStatusContributionItem"
                  id="at.medevit.elexis.agenda.ui.dynamic.status">
            </dynamic>
         </menu>
         <command
               commandId="at.medevit.elexis.agenda.ui.command.edit"
               icon="icon://IMG_EDIT"
               label="bearbeiten"
               style="push">
         </command>
         <command
               commandId="at.medevit.elexis.agenda.ui.command.delete"
               icon="icon://IMG_DELETE"
               label="löschen"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:at.medevit.elexis.agenda.ui.parallel">
         <menu
               label="Status">
            <dynamic
                  class="at.medevit.elexis.agenda.ui.menu.SetStatusContributionItem"
                  id="at.medevit.elexis.agenda.ui.dynamic.status">
            </dynamic>
         </menu>
         <command
               commandId="at.medevit.elexis.agenda.ui.command.edit"
               icon="icon://IMG_EDIT"
               label="bearbeiten"
               style="push">
         </command>
         <command
               commandId="at.medevit.elexis.agenda.ui.command.delete"
               icon="icon://IMG_DELETE"
               label="löschen"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:at.medevit.elexis.agenda.ui.view.agenda">
         <command
               commandId="at.medevit.elexis.agenda.ui.command.settopcontrol"
               label="Tag"
               style="push">
            <parameter
                  name="at.medevit.elexis.agenda.ui.command.parameter.controlId"
                  value="parallel">
            </parameter>
         </command>
         <command
               commandId="at.medevit.elexis.agenda.ui.command.settopcontrol"
               label="Woche"
               style="push">
            <parameter
                  name="at.medevit.elexis.agenda.ui.command.parameter.controlId"
                  value="week">
            </parameter>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="at.medevit.elexis.agenda.ui.handler.SetStatusHandler"
            commandId="at.medevit.elexis.agenda.ui.command.setStatus">
      </handler>
      <handler
            class="at.medevit.elexis.agenda.ui.handler.EditHandler"
            commandId="at.medevit.elexis.agenda.ui.command.edit">
      </handler>
      <handler
            class="at.medevit.elexis.agenda.ui.handler.DeleteHandler"
            commandId="at.medevit.elexis.agenda.ui.command.delete">
      </handler>
      <handler
            class="at.medevit.elexis.agenda.ui.handler.SetTopControlHandler"
            commandId="at.medevit.elexis.agenda.ui.command.settopcontrol">
      </handler>
   </extension>
</plugin>
